<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.frost.bowei_countbook.FurtherEdit">

    <TextView
        android:id="@+id/textView9"
        android:layout_width="172dp"
        android:layout_height="37dp"
        android:text="comment"
        android:textSize="24sp"
        android:layout_marginTop="16dp"
        app:layout_constraintTop_toBottomOf="@+id/editText4"
        android:layout_marginLeft="94dp"
        app:layout_constraintLeft_toLeftOf="parent" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="267dp"
        android:layout_height="32dp"
        android:text="initial value"
        android:textSize="24sp"
        android:layout_marginTop="13dp"
        app:layout_constraintTop_toBottomOf="@+id/editText5"
        android:layout_marginLeft="59dp"
        app:layout_constraintLeft_toLeftOf="parent" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="276dp"
        android:layout_height="37dp"
        android:text="Current value"
        android:textSize="24sp"
        android:layout_marginTop="17dp"
        app:layout_constraintTop_toBottomOf="@+id/textView8"
        android:layout_marginLeft="53dp"
        app:layout_constraintLeft_toLeftOf="parent" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="243dp"
        android:layout_height="52dp"
        android:layout_marginTop="16dp"
        android:text="TextView"
        android:textAlignment="center"
        android:textSize="30sp"
        android:textStyle="bold"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginLeft="59dp"
        app:layout_constraintLeft_toLeftOf="parent" />

    <EditText
        android:id="@+id/editText4"
        android:layout_width="200dp"
        android:layout_height="56dp"
        android:layout_marginLeft="66dp"
        android:layout_marginTop="16dp"
        android:ems="10"
        android:inputType="textPersonName"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView11" />

    <EditText
        android:id="@+id/editText6"
        android:layout_width="200dp"
        android:layout_height="56dp"
        android:layout_marginLeft="59dp"
        android:layout_marginTop="17dp"
        android:ems="10"
        android:hint="@string/comment"
        android:inputType="textPersonName"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView9" />

    <EditText
        android:id="@+id/editText5"
        android:layout_width="200dp"
        android:layout_height="56dp"
        android:layout_marginLeft="59dp"
        android:layout_marginTop="10dp"
        android:ems="10"
        android:inputType="textPersonName"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView10" />

    <Button
        android:id="@+id/button8"
        android:layout_width="91dp"
        android:layout_height="44dp"
        android:onClick="onClick"

        android:text="confirm"
        app:layout_constraintBaseline_toBaselineOf="@+id/editText4"
        android:layout_marginLeft="266dp"
        app:layout_constraintLeft_toLeftOf="parent" />

    <Button
        android:id="@+id/button12"
        android:layout_width="91dp"
        android:layout_height="44dp"
        android:onClick="onClick"

        android:text="confirm"
        app:layout_constraintBaseline_toBaselineOf="@+id/editText6"
        android:layout_marginLeft="266dp"
        app:layout_constraintLeft_toLeftOf="parent" />

    <Button
        android:id="@+id/button11"
        android:layout_width="91dp"
        android:layout_height="44dp"
        android:onClick="onClick"

        android:text="confirm"
        app:layout_constraintBaseline_toBaselineOf="@+id/editText5"
        android:layout_marginLeft="266dp"
        app:layout_constraintLeft_toLeftOf="parent" />

    <Button
        android:id="@+id/button13"
        android:layout_width="225dp"
        android:layout_height="43dp"
        android:text="Finish"
        android:onClick="onClick"

        android:layout_marginTop="21dp"
        app:layout_constraintTop_toBottomOf="@+id/editText6"
        android:layout_marginRight="76dp"
        app:layout_constraintRight_toRightOf="parent" />
</android.support.constraint.ConstraintLayout>
